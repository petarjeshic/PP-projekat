program P
const int size = 10;
bool dupla;
class Table {
 int pos[], neg[]; 
  {
  	void putp(int a, int idx) {
   		pos[idx] = a;
  	}
  	void putn(int a, int idx) {
   		neg[idx] = a;
  	}
  	int getp(int idx) {
   		return pos[idx];
  	}
  	int getn(int idx) {
   		return neg[idx];
  	}
 }
}
Table val; 
{
 	static void f(char ch, int a) int x; {
  			x = a;
 		}
